{
    "name":         "dexamped/kohana-aws",
    "description":  "Kohana wrapper for the aws-php-sdk.",
    "license":      "BSD-3-Clause",
    "type":         "kohana-module",
    "keywords":     ["aws", "aws-php-sdk", "kohana"],
    "authors": [
        {
            "name": "Chris",
            "email": "dexamped@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "composer/installers":  "~1.0",
        "kohana/core":          "3.3.*",
        "php":                  ">=5.3.3",
        "aws/aws-sdk-php":      "2.8.6"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:kohana-module"]
        }
    }
}
